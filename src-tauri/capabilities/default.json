{
  "$schema": "https://raw.githubusercontent.com/nicegui-org/nicegui/main/examples/tauri/src-tauri/gen/schemas/capabilities.json",
  "identifier": "default",
  "description": "Default capabilities for WSL Nexus",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-minimize",
    "core:window:allow-toggle-maximize",
    "core:window:allow-close",
    "core:window:allow-hide",
    "core:window:allow-is-maximized",
    "core:window:allow-start-dragging",
    "shell:allow-open",
    "shell:allow-execute",
    "shell:default",
    "store:default",
    "dialog:default",
    "notification:default"
  ]
}
